// app global css
.note
  border-radius 4px
  margin 16px 0
  padding 16px 24px
  font-size 1em
  border-color #9e9e9e
  border-style solid
  border-width 0 5px
  letter-spacing .5px

.note--warning
  background-color #f9e09b
  border-color #f2c037

.note--danger
  background-color #ffc0c6
  border-color #c10015
